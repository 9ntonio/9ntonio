{
	"name": "antonio-almena-portfolio",
	"description": "Antonio Almena Portfolio",
	"version": "1.1.0",
	"homepage": "https://antonio.almena.io",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/9ntonio/9ntonio"
	},
	"keywords": [
		"tsparticles",
		"particles",
		"particles.js",
		"react-particles-js",
		"react",
		"react.js",
		"reactjs",
		"react-particles.js",
		"react-tsparticles",
		"react-particles",
		"particlesjs",
		"particle",
		"jsparticles",
		"xparticles",
		"particles-js",
		"particles-bg",
		"particles-bg-vue",
		"particles-ts",
		"particles.ts",
		"typescript",
		"javascript",
		"animation",
		"web",
		"html5",
		"web-design",
		"webdesign",
		"css",
		"html",
		"css3",
		"animated",
		"background",
		"gatsby",
		"gatsby-theme",
		"unknown-pleasures",
		"joy-division"
	],
	"scripts": {
		"dev": "gatsby develop",
		"format": "prettier --write \"**/*.{js,jsx,json,md}\"",
		"lint": "eslint \"src/**/*.{js,jsx}\" --fix",
		"lint:check": "eslint \"src/**/*.{js,jsx}\"",
		"start": "yarn dev",
		"clean": "gatsby clean",
		"build": "yarn build:modern && yarn postbuild && yarn analyze-bundle",
		"build:modern": "NODE_ENV=production GATSBY_MODERN_BUILD=true GATSBY_CPU_COUNT=2 GATSBY_CONTINUE_BUILD_ON_MISSING_ADAPTER=true gatsby build",
		"build:legacy": "NODE_ENV=production GATSBY_MODERN_BUILD=false GATSBY_CPU_COUNT=2 GATSBY_CONTINUE_BUILD_ON_MISSING_ADAPTER=true gatsby build",
		"build:differential": "yarn clean && yarn build:modern && yarn build:legacy && yarn postbuild:differential",
		"build:production": "yarn build:modern && yarn postbuild",
		"serve": "gatsby serve",
		"analyze-bundle": "node scripts/analyze-bundle.js",
		"minification-report": "node scripts/minification-report.js",
		"postbuild": "node scripts/fix-unknown-pleasures.js",
		"postbuild:differential": "node scripts/differential-build-processor.js && node scripts/fix-unknown-pleasures.js",
		"fix-unknown-pleasures": "node scripts/fix-unknown-pleasures.js",
		"verify-build": "node scripts/verify-build.js",
		"validate-modern-build": "node scripts/validate-modern-build.js",
		"compare-builds": "node scripts/compare-builds.js",
		"validate-performance": "node scripts/validate-performance.cjs",
		"pre-commit": "npm run format && npx lint-staged",
		"prepare": "husky install"
	},
	"dependencies": {
		"@babel/core": "^7.22.0",
		"@fortawesome/fontawesome-common-types": "^6.7.2",
		"@fortawesome/fontawesome-svg-core": "^6.5.1",
		"@fortawesome/free-brands-svg-icons": "^6.5.1",
		"@fortawesome/free-regular-svg-icons": "^6.5.1",
		"@fortawesome/free-solid-svg-icons": "^6.5.1",
		"@fortawesome/react-fontawesome": "^0.2.0",
		"@gatsbyjs/reach-router": "^2.0.0",
		"@netlify/plugin-gatsby": "^3.8.1",
		"@popperjs/core": "^2.11.8",
		"common-tags": "^1.8.2",
		"express": "^5.0.0",
		"fs-extra": "^11.2.0",
		"gatsby": "^5.14.1",
		"gatsby-adapter-netlify": "^1.2.0",
		"gatsby-plugin-google-fonts": "^1.0.1",
		"gatsby-plugin-gtag": "^1.0.13",
		"gatsby-plugin-image": "^3.12.3",
		"gatsby-plugin-manifest": "^5.14.0",
		"gatsby-plugin-netlify": "^5.1.1",
		"gatsby-plugin-postcss": "^6.13.0",
		"gatsby-plugin-preload-fonts": "^4.14.0",
		"gatsby-plugin-sharp": "5.14.0",
		"gatsby-plugin-sitemap": "^6.14.0",
		"gatsby-source-filesystem": "5.14.0",
		"gatsby-transformer-sharp": "5.14.0",
		"prop-types": "^15.8.1",
		"react": "18.3.1",
		"react-dom": "18.3.1",
		"react-helmet": "^6.1.0",
		"react-player": "2.16.1",
		"react-tsparticles": "^2.12.2",
		"sass": "^1.55.0",
		"sharp": "^0.34.0",
		"tsparticles": "^3.0.0",
		"tsparticles-preset-triangles": "^2.12.0",
		"webpack": "^5.88.0"
	},
	"devDependencies": {
		"@babel/plugin-proposal-nullish-coalescing-operator": "^7.18.6",
		"@babel/plugin-proposal-optional-chaining": "^7.21.0",
		"@babel/plugin-syntax-dynamic-import": "^7.8.3",
		"@babel/preset-env": "^7.28.3",
		"@babel/preset-react": "^7.27.1",
		"autoprefixer": "^10.4.16",
		"babel-plugin-transform-imports": "^2.0.0",
		"babel-plugin-transform-react-remove-prop-types": "^0.4.24",
		"core-js": "^3.45.0",
		"eslint": "^9.0.0",
		"eslint-config-prettier": "^10.0.0",
		"eslint-plugin-gatsby": "^1.0.2",
		"eslint-plugin-jsx-a11y": "^6.8.0",
		"eslint-plugin-react": "^7.33.2",
		"eslint-plugin-react-hooks": "^4.6.0",
		"gatsby-plugin-minify": "^0.5.0",
		"gatsby-plugin-remove-serviceworker": "^1.0.0",
		"gatsby-plugin-webpack-bundle-analyser-v2": "^1.1.32",
		"glob": "^11.0.3",
		"husky": "^9.0.0",
		"lint-staged": "^16.0.0",
		"node-addon-api": "^8.3.1",
		"postcss": "^8.4.32",
		"postcss-loader": "^8.0.0",
		"prettier": "^3.0.0",
		"tailwindcss": "^3.4.0",
		"terser": "^5.43.1",
		"terser-webpack-plugin": "^5.3.14"
	},
	"lint-staged": {
		"*.{js,jsx}": [
			"eslint --fix",
			"prettier --write"
		],
		"*.{json,md,css,scss}": [
			"prettier --write"
		]
	}
}
